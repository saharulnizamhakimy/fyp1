@model IEnumerable<fyp1.Models.tb_acadprog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ap_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ap_desc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ap_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ap_desc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ap_ID }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Details", "Details", new { id=item.ap_ID }, new { @class = "btn btn-info" }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ap_ID }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
