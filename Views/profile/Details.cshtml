@model fyp1.Models.tb_user

@{
    ViewBag.Title = "Profile Details";
}

<h2>Profile Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">


        <dt>
            @Html.DisplayNameFor(model => model.u_name)
        </dt>

        <dd>
            @Html.EditorFor(model => model.u_name, new { htmlAttributes = new { @class = "form-control", @readonly = "True" } })
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.u_pwd)
        </dt>

        <dd>
            @Html.TextBoxFor(model => model.u_pwd, new { @class = "form-control", type = "password", @readonly = "true" })
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.u_contact)
        </dt>

        <dd>
            @Html.EditorFor(model => model.u_contact, new { htmlAttributes = new { @class = "form-control", @readonly = "True" } })

        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.u_email)
        </dt>

        <dd>
            @Html.EditorFor(model => model.u_email, new { htmlAttributes = new { @class = "form-control", @readonly = "True" } })
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.u_acadProgID)
        </dt>

        <dd>
            @Html.EditorFor(model => model.u_acadProgID, new { htmlAttributes = new { @class = "form-control", @readonly = "True" } })
            @*@Html.DisplayFor(model => model.tb_student.s_acadprogID)*@
        </dd>



    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.u_ID })
    @*@Html.ActionLink("Back to List", "Index")*@
</p>
